{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "name": "countryList",
      "databaseName": "factbook",
      "collectionName": "Country",
      "connectionStringSetting": "factbook_DOCUMENTDB",
      "direction": "in",
      "partitionKey": "Factbook",
      "sqlQuery": "SELECT top 10 c.name, c.key, c.economy.inflation_rate.annual_values[0], c.economy.inflation_rate.global_rank FROM c order by c.economy.inflation_rate.global_rank asc"
    }
  ]
}